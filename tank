  function checkCMGDefaultExceptionURL() {
    supportCMG = false;
    if(!cmgDefaultGameUrl.test(location.pathname) && (typeof gamesPlayed != "undefined" && (gamesPlayed == 2 || gamesPlayed == 8))) {
