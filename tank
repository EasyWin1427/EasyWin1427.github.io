	              type: "med_rect_btf"
	            },
	            {
	              selectorId: "sky-btf",
	              type: "sky_btf"
	            }
	          ];
					} else {  //desktop
						pwUnits = [{
	              selectorId: "leaderboard-atf",
	              type: "leaderboard_atf"
	            },
	            {
	              selectorId: "leaderboard-btf",
	              type: "leaderboard_btf"
	            },
